{
  "name": "color-space-shift",
  "displayName": "color-space-shift",
  "description": "Change color spaces from Hex to HSL and more.",
  "version": "0.0.1",
  "publisher": "bbugh",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.colorSpaceShift.hslSmartConvert"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.colorSpaceShift.hslSmartConvert",
        "title": "Convert Color to HSL/A"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "format": "prettier --write src/**/*.ts test/**/*.ts",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.13.7",
    "mocha": "^6.1.4",
    "prettier": "^1.17.0",
    "typescript": "^3.4.5",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "@types/color": "^3.0.0",
    "color": "^3.1.1"
  }
}
